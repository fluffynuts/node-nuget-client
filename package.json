  },
  "homepage": "https://github.com/fluffynuts/node-nuget-client#readme",
  "devDependencies": {
    "@types/bent": "^7.0.2",
    "@types/jest": "^25.2.3",
    "@types/node": "^13.13.4",
    "@types/rimraf": "^3.0.0",
    "@types/unzipper": "^0.10.3",
    "expect-even-more-jest": "^1.3.0",
    "filesystem-sandbox": "^1.11.0",
    "jest": "^26.0.1",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^26.1.0",
